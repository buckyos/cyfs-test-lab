{
  "compilerOptions": {
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
        "@src/*": ["./*"],
        "@www/*": ["./www/*"]
    },
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "sourceMap": false,
    "outDir": "./dist",
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["./", "typing.d.ts"],
  "exclude": ["deploy", "node_modules"]
}
